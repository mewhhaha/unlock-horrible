{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "pnpm build && wrangler deploy",
    "dev": "pnpm dev:assets && pnpm dev:wrangler",
    "start": "wrangler dev",
    "test": "vitest",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc -p tsconfig.json",
    "routes": "fs-routes ./app",
    "build": "run-s build:*",
    "build:routes": "pnpm routes",
    "build:styles": "tailwindcss -i ./app/tailwind.css -o ./app/assets/styles.css",
    "build:assets": "fingerprint -i ./app/assets -o ./dist/assets -m ./app/import-map.json",
    "dev:wrangler": "wrangler dev --assets ./app/assets --live-reload",
    "dev:styles": "tailwindcss -i ./app/tailwind.css -o ./app/assets/styles.css --watch",
    "dev:assets": "fingerprint -i ./app/assets -o ./dist/assets -m ./app/import-map.json --dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@mewhhaha/eslint-plugin-simple-tailwind": "^0.0.12",
    "@packages/fingerprint": "workspace:^",
    "@packages/passq": "workspace:^",
    "@mewhhaha/htmx-router": "workspace:^",
    "@packages/jwt": "workspace:^",
    "@tailwindcss/cli": "4.0.0-beta.8",
    "@types/cookie-signature": "^1.1.2",
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-oxlint": "^0.15.2",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.15.3",
    "prettier": "^3.4.2",
    "tailwindcss": "4.0.0-beta.8",
    "tailwindcss-motion": "^1.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "@passwordless-id/webauthn": "^2.1.2",
    "arktype": "2.0.0-rc.33",
    "cookie": "^1.0.2",
    "cookie-signature": "^1.2.2"
  }
}
