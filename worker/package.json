{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "rimraf dist/assets && pnpm build && wrangler deploy",
    "test": "vitest",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc -p tsconfig.json",
    "routes": "fs-routes ./app",
    "build": "NODE_ENV=production rolldown -c ./rolldown.config.js --minify",
    "dev": "run-p dev.rebuild dev.wrangler",
    "dev.wrangler": "wrangler dev --live-reload",
    "dev.rebuild": "rolldown -c ./rolldown.config.js --minify --watch"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250121.0",
    "@mewhhaha/eslint-plugin-simple-tailwind": "^0.0.12",
    "@mewhhaha/htmx-router": "workspace:^",
    "@packages/passkey": "workspace:^",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/node": "^4.0.0",
    "@tailwindcss/oxide": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-oxlint": "^0.15.7",
    "fast-glob": "^3.3.3",
    "lightningcss": "^1.29.1",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.15.7",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.2-commit.f069c5d",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.21.0",
    "vitest": "^3.0.3",
    "wrangler": "^3.103.2"
  },
  "dependencies": {
    "@passwordless-id/webauthn": "^2.1.2",
    "cookie": "^1.0.2"
  }
}
